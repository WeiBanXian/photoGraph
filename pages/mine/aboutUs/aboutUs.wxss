.wrapper {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background: #F9F9F9;
}

.logo {
    display: block;
    width: 120rpx;
    height: 120rpx;
}

.name {
    display: block;
    margin: 20rpx;
    font-size: 24rpx;
    color: #333;
}

.version {
    font-size: 24rpx;
    color: #333;
}

.botWrapper {
    position: absolute;
    width: 100%;
    bottom: 10rpx;
    text-align: center;
}

.protocol {
    font-size: 24rpx;
    color: #333;
}

.email {
    display: block;
    font-size: 24rpx;
    color: #333;
}