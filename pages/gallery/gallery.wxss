.galleryWrapper {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background: #FF0;
}

.blockWrapper {
    background: #F9F9F9;
}

.picsWrapper {
    margin: 0;
    padding: 0;
    display: flex;
    flex-wrap: wrap;
}

.pic {
    display: inline-block;
    position: relative;
    width: 248rpx;
    height: 248rpx;
    margin: 0;
    padding: 0;
    margin-left: 2rpx;
    margin-bottom: 2rpx;
    background-image: url("../../resource/images/logoBg.png");
    background-size: 100% 100%;
    background-color: #EEE;
}

.emptyWrapper {
    display: flex;
    width: 100%;
    height: 100%;
    justify-content: center;
    align-items: center;
    background: #F9F9F9;
}

.empty {
    width: 20%;
}