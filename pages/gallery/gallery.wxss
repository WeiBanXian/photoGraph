.galleryWrapper {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background: #FF0;
}

.deleteWrapper {
    position: fixed;
    right: 10rpx;
    bottom: 10rpx;
}

.blockWrapper {
    background: #F9F9F9;
}

.picsWrapper {
    margin: 0;
    padding: 0;
    display: flex;
    flex-wrap: wrap;
}

.pic {
    display: inline-block;
    position: relative;
    width: 248rpx;
    height: 248rpx;
    margin: 0;
    padding: 0;
    margin-left: 2rpx;
    margin-bottom: 2rpx;
    background-image: url("../../resource/images/logoBg.png");
    background-size: 100% 100%;
    background-color: #EEE;
}

.cancelWrapper {
    position: absolute;
    top: 0;
    right: 0;
}

.emptyWrapper {
    display: flex;
    width: 100%;
    height: 100%;
    justify-content: center;
    align-items: center;
    background: #F9F9F9;
}

.empty {
    width: 20%;
}

.logoutWrapper {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background: #EEE;
}

.logo {
    margin: 30rpx 0;
    width: 160rpx;
    height: 160rpx;
}

.logoutBtn {
    display: block;
    margin-top: 30rpx;
    width: 480rpx;
    height: 75rpx;
    line-height: 75rpx;
    font-size: 36rpx;
}

.pullLoad {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 80rpx;
    background: #F9F9F9;
}

.loadLogo {
    width: 60rpx;
    height: 60rpx;
}

.loadText {
    margin-left: 15rpx;
    font-size: 28rpx;
    color: #666;
}